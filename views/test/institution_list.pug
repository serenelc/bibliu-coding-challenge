extends layout-test

block content
  h1= title
  
  ul
    - institution_list.sort(function(a, b) {let textA = a.name.toUpperCase(); let textB = b.name.toUpperCase(); return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;});
    each institution in institution_list
      li 
        a(href='/test/institution/' + institution.id) #{institution.name}

    else
      li There are no users.